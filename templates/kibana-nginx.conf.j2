server {
    listen 0.0.0.0:80;
    server_name localhost;

    listen 0.0.0.0:443 ssl;
    return 301 https://$server_name$request_uri;

    ssl_certificate /etc/ssl/freedom.crt;
    ssl_certificate_key /etc/ssl/freedom.key;

    location / {
        proxy_pass http://127.0.0.1:5601/;
        proxy_redirect off;
        proxy_buffering off;
    }
}
